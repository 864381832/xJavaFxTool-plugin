<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xwintop.xJavaFxTool.controller.developTools.xTransferTool.TransferToolDataSourceController">
   <children>
      <SplitPane fx:id="dataSourceSplitPane" orientation="VERTICAL" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <items>
            <FlowPane fx:id="dataSourceFlowPane" hgap="20.0" vgap="10.0">
               <children>
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveAction" text="保存" />
                  <Button fx:id="viewButton" mnemonicParsing="false" onAction="#viewAction" text="预览" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </FlowPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
